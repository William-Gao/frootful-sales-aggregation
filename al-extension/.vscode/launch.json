{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Publish to BC Production",
            "type": "al",
            "request": "launch",
            "environmentType": "OnPrem",
            "server": "https://your-bc-server.com",
            "serverInstance": "BC190",
            "tenant": "default",
            "authentication": "Windows",
            "startupObjectId": 50100,
            "startupObjectType": "Page",
            "breakOnError": true,
            "launchBrowser": true,
            "enableLongRunningSqlStatements": true,
            "enableSqlInformationDebugger": true,
            "schemaUpdateMode": "Synchronize"
        },
        {
            "name": "Publish to BC Online",
            "type": "al",
            "request": "launch",
            "environmentType": "Sandbox",
            "environmentName": "YourSandboxName",
            "startupObjectId": 50100,
            "startupObjectType": "Page",
            "breakOnError": true,
            "launchBrowser": true,
            "schemaUpdateMode": "Synchronize"
        }
    ]
}